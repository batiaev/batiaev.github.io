{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-console": ["warn"],
    "max-depth": ["error", 2],

    "import/no-duplicates": "error",

    "padding-line-between-statements": [
      "error",
      {
        "blankLine": "always",
        "prev": ["block", "block-like", "export", "import"],
        "next": "*"
      },

      { "blankLine": "any", "prev": ["export", "import"], "next": ["export", "import"] }
    ]
  }
}
